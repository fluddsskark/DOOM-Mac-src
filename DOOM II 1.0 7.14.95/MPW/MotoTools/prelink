set oldexit {exit}set exit 0set temp "{TempFolder}"¥¥prelink-temp¥¥# The maximum number of iterations after which we give up under the# assumption that we've encountered an instantiation loopset count 30set cwd `execute directory`loop  set count `evaluate {count} - 1`  if {count}<=0  	echo "Instantation loop"	break  end  prelink_cmd {"parameters"} > "{temp}"  if {status}!=0    break  end  if `count -c "{temp}"` == 0    break  end  #catenate "{temp}"  "{temp}"  if {status}!=0    break  endenddirectory "{cwd}"delete -i -y "{temp}"