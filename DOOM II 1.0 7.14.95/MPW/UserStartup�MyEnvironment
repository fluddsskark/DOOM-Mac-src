#========================== Root/Work ======================================Set jmRootDisk "Leozinho:"Export jmRootDiskSet jmDevDisk ""{jmRootDisk}"Development:"Export jmDevDisk#========================== Projector   ======================================Set jmProjectorDisk ""{jmRootDisk}"Projects:"Export jmProjectorDisk#========================= Doom Project ======================================Set jmDoomProjectDisk ""{jmDevDisk}"Doom:"Export jmDoomProjectDiskSet jmDoomProjectorDisk ""{jmProjectorDisk}"DoomProj:"Export jmDoomProjectorDiskSet jmDoomProject DoomProjºExport jmDoomProject#========================= Doom Project ======================================Set jmDoomIIProjectDir ""{jmDevDisk}"DoomII:"Export jmDoomIIProjectDirSet jmDoomIISourcesDir ""{jmDoomIIProjectDisk}"Sources:"Export jmDoomIISourcesDir########################## CURRENT DEFAULT STUFF #############################Set jmCurrentProjectDisk "{jmDoomProjectDisk}"Export jmCurrentProjectDiskSet ProjectorUsed 0Export ProjectorUsedSet jmCurrentProjectorDisk "{jmDoomProjectorDisk}"Export jmCurrentProjectorDisk# Other directories to be inserted into directory menuSet jmAltDir1 "{jmDoomIIProjectDir}"Export jmAltDir4Set jmAltDir2 "{jmDoomIISourcesDir}"Export jmAltDir5