Fixed_DIV_68K	PROC 	EXPORT	MACHINE	MC68030;_________________________________________________________________________; Function Fixed_MUL_68K(mul1, mul2: long): long;StackFrame     RECORD    {A6Link},DECR     ; build a stack frame recordResult1        DS.L      1                 ; function's result returned to callerParamBegin     EQU       *                 ; start parameters after this pointdiv1           DS.L      1                 ; dividend parameterdiv2           DS.L      1                 ; divisor parameterParamSize      EQU       ParamBegin-*      ; size of all the passed parametersRetAddr        DS.L      1                 ; placeholder for return addressA6Link         DS.L      1                 ; placeholder for A6 linkLocalSize      EQU       *                 ; size of all the local variables               ENDR			       WITH       StackFrame		LINK	A6,#LocalSize		MOVEM.L		D0-D2,-(A7); copy parameters to registers	MOVE.L	div1(A6),D0	MOVE.L	div2(A6),D2; perform the division	SWAP	D0	MOVE.W	D0,D1	EXT.L	D1	CLR.W	D0	DIVS.L	D2,D1:D0	; assign result and return	MOVE.L     D0,Result1(A6)			; Return status	MOVEM.L		(A7)+,D0-D2	UNLK       A6						; destroy the link	MOVEA.L    (SP)+,A0					; pull off the return address	ADDA.L     #ParamSize,SP			; strip all of the caller's parameters	JMP        (A0)						; return to the caller		ENDWITH	ENDP	END