Fixed_MUL_68K	PROC 	EXPORT	MACHINE	MC68030;_________________________________________________________________________; Function Fixed_MUL_68K(mul1, mul2: long): long;StackFrame     RECORD    {A6Link},DECR     ; build a stack frame recordResult1        DS.L      1                 ; function's result returned to callerParamBegin     EQU       *                 ; start parameters after this pointmul1           DS.L      1                 ; first multiplicand parametermul2           DS.L      1                 ; second multiplicand parameterParamSize      EQU       ParamBegin-*      ; size of all the passed parametersRetAddr        DS.L      1                 ; placeholder for return addressA6Link         DS.L      1                 ; placeholder for A6 linkLocalSize      EQU       *                 ; size of all the local variables               ENDR			       WITH       StackFrame		LINK	A6,#LocalSize		MOVEM.L		D0-D2,-(A7); copy mulitplicand to registers	MOVE.L	mul1(A6),D2	MOVE.L	mul2(A6),D0; perform the multiplication	MULS.L	D2,D1:D0	MOVE.W	D1,D0	SWAP	D0	; assign result and return	MOVE.L     D0,Result1(A6)			; Return status	MOVEM.L		(A7)+,D0-D2	UNLK       A6						; destroy the link	MOVEA.L    (SP)+,A0					; pull off the return address	ADDA.L     #ParamSize,SP			; strip all of the caller's parameters	JMP        (A0)						; return to the caller		ENDWITH	ENDP	END