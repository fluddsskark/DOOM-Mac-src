{	Example.p		A simple profiler example}program Example;uses	Types,	OSUtils,	Profiler;		procedure test4;		var		i, j	:integer;		begin {test4}		j := 0;		for i := 0 to 99 do			j := j + 1;	end; {test4}	procedure test3(c :integer);		var		t	:LongInt;		begin {test3}		Delay(c, t);		test4;	end; {test3}		function test2(b: integer) :integer;		var		t:	LongInt;		begin {test2}		Delay(b, t);		test3(b);		test4;				test2 := b;	end; {test2}	procedure test(a :integer);		var		i:	integer;		begin {test}		for i := a-1 downto 0 do			if test2(5) <> 5 then				SysBeep(2);	end; {test}var	error:	OSErr;{  If you copy and paste the following line from the example, be sure to  make sure that you change the function table size and the stack table  size passed to ProfilerInit to appropriate values for your application.}begin {example}	if ProfilerInit(collectDetailed, bestTimeBase, 20, 5) = noErr then begin		test(15);		error := ProfilerDump('Example.prof');		ProfilerTerm;	end;end. {example}