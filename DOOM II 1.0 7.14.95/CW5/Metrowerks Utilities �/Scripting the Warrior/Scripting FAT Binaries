set PATHNAME to "CW5 Gold:CodeWarrior Examples Ä:C/C++ on Power Mac:ModApp Ä:"set BUILD to "OFF"with timeout of 60000 seconds	tell application "MW C/C++ 68K 1.2"		activate		open {PATHNAME & "ModApp.68K.¹"}		if BUILD is "ON" then			Make Project		else			Remove Binaries		end if	end tell		tell application "MW C/C++ PPC 1.2"		activate		open {PATHNAME & "ModApp.PPC.¹"}		if BUILD is "ON" then			Make Project		else			Remove Binaries		end if				open {PATHNAME & "GWorldTools.¹"}		if BUILD is "ON" then			Make Project		else			Remove Binaries		end if				open {PATHNAME & "Button.¹"}		if BUILD is "ON" then			Make Project		else			Remove Binaries		end if				open {PATHNAME & "Clock.¹"}		if BUILD is "ON" then			Make Project		else			Remove Binaries		end if				open {PATHNAME & "Koch.¹"}		if BUILD is "ON" then			Make Project		else			Remove Binaries		end if				open {PATHNAME & "Simple.¹"}		if BUILD is "ON" then			Make Project		else			Remove Binaries		end if				open {PATHNAME & "PowerResource.¹"}		if BUILD is "ON" then			Make Project		else			Remove Binaries		end if			end tell	end timeout